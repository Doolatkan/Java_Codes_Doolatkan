package day14_methods_Part2;



public class Task69_PrimeNumber {

	public static void main(String[] args) {
		
		
		for(int i=0;i<=100;i++) {
			
		if(i%2==0) {
			if(i==2) {
				System.out.println(i + " prime");
			}
		}else if(i>2) {
			if(i%3==0)
			System.out.println(i + "pp");
		}

		
		
		
		
		
		
		
		
		
		
		
		
	}
	
}
}
