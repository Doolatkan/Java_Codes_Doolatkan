package day14_methods_Part2;



public class Task69_PrimeNumber {

	public static void main(String[] args) {
    
      boolean prime = false;
      for(int num=0;num<=100;num++) {
    	  
  		if(num>10){
  			int num1 = num%100;
  			int num2 = num1/10;
  			int num3 = num%10;
  			int num4 = num2+num3;
  			
  			if(num4%3!=0) {
  				prime = false;
  	  			System.out.print(num+" ");
  			}else  {
  	  			
  	  			prime=true;
  	  			System.err.print(num+" ");
  	  		}
  		}else if(num>3 && num%3==0||(num!=2 && num%2==0) ) {  			
  			prime = false;
  			System.out.print(num+" ");
  		}else  {  			
  			prime=true;
  			System.err.print(num+" ");
  		}
      
  		
  		}

	}



}
