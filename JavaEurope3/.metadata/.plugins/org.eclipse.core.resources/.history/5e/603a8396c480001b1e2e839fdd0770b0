package day20_stringManipulation_part2;

public class String_Replace {

	public static void main(String[] args) {
		
		String str = "Moscow is a capital city of Russia (of Russia)";
		System.out.println(str);
		
		str = str.replace("Moscow", "Bishkek").replace("Russia", "Kyrgyzstan").replace("capital", "hlavni");
		System.out.println(str);
		
		str = str.replace("Bishkek", "Praha").replace("Praha", "Brno").replace("Brno", "PRAHA").replace("Kyrgyzstan", "Czech");
		System.out.println(str);
		
		str = str.replace("Chech Republic", "CZECH");//if cannot finde so it is will not be change
		System.out.println(str);
		
		
		
		String email = "firstName_LastName@gmail.com";
		String company = "capitalone";
		
		String newEmail = email.replace("gmail", company);
		System.out.println(newEmail);
		System.out.println(newEmail.replace("o", "a"));
		System.out.println(email);
		
		

	}

}
