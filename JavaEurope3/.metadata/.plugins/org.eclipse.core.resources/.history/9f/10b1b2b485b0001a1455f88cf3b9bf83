package day14_methods_Part2;



public class Task69_PrimeNumber {

	public static void main(String[] args) {
		
		
		for(int i=0;i<=100;i++) {
			if(i%2==0) {
				if(i==2) {
					System.err.println("         " + i + " prime");
				}else {
					System.out.println(i + " no prime");
				}
				
			}else if(i+2%3==0 && i!=3 || i!=3 ) {
				System.out.println(i + " no prime");
			}else {
				System.err.println("         " + i + " prime");
			}
	}
	

	}
	}
