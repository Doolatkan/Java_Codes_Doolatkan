package day20_stringManipulation_part2;

public class String_IndexOf2 {

	public static void main(String[] args) {
		
		//index of with 2 values
		
		String list = "html-selenium-angular-jenkins-grid";
		
		int firstDash = list.indexOf("-");
		System.out.println(firstDash);
		System.out.println(list.substring(firstDash));
		
		int secondDash = list.indexOf("-", 5);
		System.out.println(secondDash);
		
		int thirdDash = list.indexOf("-", secondDash+1);
		System.out.println(thirdDash);
		
		int lastDash = list.lastIndexOf("-");
		System.out.println(lastDash);
		
		int preLastDash = list.lastIndexOf("-", 28);
		System.out.println(list.charAt(preLastDash));//
		System.out.println(preLastDash);
		
		
		int preLastDash1 = list.lastIndexOf("-", 20);
		System.out.println(preLastDash1);
		
		int preLastDash2 = list.lastIndexOf("-", 12);
		System.out.println(preLastDash2);
		
		int preLastDash3 = list.lastIndexOf("-", 3);
		System.out.println(preLastDash3);
		
		

	}

}
